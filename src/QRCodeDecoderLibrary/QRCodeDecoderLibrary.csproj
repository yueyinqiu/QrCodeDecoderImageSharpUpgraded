<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net9.0</TargetFrameworks>
        <Description>This QR Code library supports decoding a QR Code image using ImageSharp</Description>
        <RootNamespace>QRCodeDecoderLibrary</RootNamespace>
        <LangVersion>9</LangVersion>
        <PackageId>QRCodeDecoder-ImageSharp</PackageId>
        <PackageLicenseUrl>https://www.codeproject.com/info/cpol10.aspx</PackageLicenseUrl>
        <UserSecretsId>2c11cc4d-9f6a-45ac-aab3-bbcab3318c04</UserSecretsId>
        <Authors>Deepak Sharma,Uzi Granot;Stef Heyenrath</Authors>
        <Product>QRCodeDecoderLibrary</Product>
        <Copyright />
        <PackageProjectUrl>https://github.com/deathrot/QRCode</PackageProjectUrl>
        <RepositoryUrl>https://github.com/deathrot/QRCode</RepositoryUrl>
        <PackageTags>QRCode, C#, Barcode, Decode, ImageSharp</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="SixLabors.ImageSharp" Version="2.1.3" />
        <PackageReference Include="Stef.Validation" Version="0.1.1" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\QRCodeSharedLibrary\QRCodeSharedLibrary.csproj" />
    </ItemGroup>

</Project>